set number
set mouse=a
set numberwidth=2
set clipboard=unnamed
syntax enable
set showcmd
set ruler
set cursorline
set encoding=utf-8
set showmatch
set sw=2
set relativenumber

runtime .vim/plugins.vim
runtime .vim/plugin-config.vim
runtime .vim/maps.vim
runtime .vim/snippets.vim

set laststatus=2
colorscheme gruvbox
